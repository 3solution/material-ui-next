{
  "extends": "../tsconfig.json",
  "include": ["next-env.d.ts", "types", "src/pages/**/*"],
  "compilerOptions": {
    "allowJs": false,
    "isolatedModules": true,
    /* files are emitted by babel */
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "types": ["react"]
  },
  "exclude": ["node_modules"]
}
