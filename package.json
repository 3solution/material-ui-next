    "**/@babel/preset-env": "^7.10.2",
    "**/@babel/preset-react": "^7.10.1",
    "**/@babel/preset-typescript": "^7.10.4",
    "**/@babel/runtime": "^7.10.2",
    "**/@babel/types": "^7.10.2",
    "**/cross-fetch": "^3.0.5",
    "**/dot-prop": "^5.2.0",
    "**/webpack": "^4.44.1"
  },
  "nyc": {
    "include": [
      "packages/material-ui/src/**/*.js",
      "packages/material-ui-utils/src/**/*.js",
      "packages/material-ui-styles/src/**/*.js"
    ],
    "exclude": [
      "**/*.test.{js,ts,tsx}"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "workspaces": [
    "benchmark",
    "packages/*",
    "docs",
    "docs/packages/*",
    "framer"
  ]
