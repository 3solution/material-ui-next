    "expect-puppeteer": "^4.3.0",
    "format-util": "^1.0.5",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.2",
    "glob-gitignore": "^1.0.11",
    "jsdom": "^16.0.0",
    "karma": "^5.0.1",
    "karma-browserstack-launcher": "~1.4.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
